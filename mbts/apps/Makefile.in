# Makefile
# This file holds the make rules for the local BSS helper

DEBUG :=

CXX  := @CXX@ -Wall
AR  := ar
DEFS :=
LIBTHR := @THREAD_LIB@
INCLUDES = $(ALL_INCLUDES)
CFLAGS := @YATE_INC@
LDFLAGS:= @LDFLAGS@
YATELIBS := @YATE_LNK@
INCFILES :=

PROGS:= mbts
LIBS :=
OBJS := GetConfigurationKeys.o
LOCALLIBS = $(ALL_LIBS) $(A53_LIBS)

all:

mbts: OpenBTS.cpp $(MKDEPS) $(INCFILES) $(OBJS) $(LIBS) $(ALL_DEPS) $(A53_DEPS)
	$(COMPILE) -o $@ $(LOCALFLAGS) $< $(OBJS) $(LIBS) $(LIBTHR) $(LDFLAGS) $(LOCALLIBS) $(YATELIBS)
